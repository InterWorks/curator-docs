r at Vistage, current Curator client. I'm reaching out to review customizations - the Vistage site uses custom styles, custom PHP and currently runs on NT.\r\nCustom PHP pasted below.\r\n\r\nThank you.\r\n\r\n<?php\r\n\r\n\/\/testing save\r\n\r\nuse InterWorks\\TableauViz\\Classes\\Tableau;\r\nuse InterWorks\\UserMgmt\\Models\\FrontendUser;\r\nuse InterWorks\\Portal\\Classes\\Cache;\r\nuse InterWorks\\Portal\\Classes\\Utils;\r\n\r\nfunction noUserError() {\r\n    echo \"<html>\";\r\n    echo '<head><link href=\"\/storage\/app\/media\/theme.css\" rel=\"stylesheet\" \/>';\r\n    echo \"<\/head><body id='saml_error'><center>\";\r\n    echo \"<center><h2>Access to Vistage Chair Reports is available to US Chairs only.<\/h2>\";\r\n    echo \"<h2>Vistage staff can access similar reports \";\r\n    echo \"<a href='http:\/\/dashboard.vistage.internal'>here<\/a>.<\/h2><\/center>\";\r\n    \r\n    exit;\r\n}\r\n\r\nfunction userMap($username, $skip_impersonator=false) {\r\n    if (empty($username)) {\r\n        noUserError();\r\n        return false;\r\n    }\r\n    \r\n    \\Session::forget('user');\r\n    $attributes = \\Session::get('saml_attributes');\r\n    \r\n    if (empty($attributes)) {\r\n        noUserError();\r\n        return false;\r\n    }\r\n    \r\n    if (empty($attributes['PartnerCode'])) {\r\n        noUserError();\r\n        return false;\r\n    }\r\n    \r\n    $partner      = strtoupper(current($attributes['PartnerCode']));\r\n    $chair        = strtoupper(current($attributes['IsChair']));\r\n    $speaker      = strtoupper(current($attributes['IsSpeaker']));\r\n    $impersonator = strtoupper(current($attributes['IsImpersonator']));\r\n    $region       = strtoupper(current($attributes['Region']));\r\n    $portal_user  = strtoupper(current($attributes['is_portal_user']));\r\n    \r\n    if ($impersonator && $impersonator != 'FALSE' && !$skip_impersonator) {\r\n        return 'portal_admin';\r\n    }\r\n    \r\n    if ($portal_user != 'TRUE') {\r\n        noUserError();\r\n        return false;\r\n    }\r\n    \r\n    if ($partner == 'SAND') {\r\n        if ($chair == 'TRUE') {\r\n            if ($speaker == 'TRUE') {\r\n                return 'portal_sand_chair_speaker';\r\n            } else {\r\n                return 'portal_sand_chair';\r\n            }\r\n        } else if ($speaker == 'TRUE') {\r\n            return 'portal_sand_speaker';\r\n        }\r\n    } else if ($partner == 'UKEN') {\r\n        if ($chair == 'TRUE') {\r\n            if ($speaker == 'TRUE') {\r\n                return 'portal_uken_chair_speaker';\r\n            } else {\r\n                return 'portal_uken_chair';\r\n            }\r\n        } else if ($speaker == 'TRUE') {\r\n            return 'portal_uken_speaker';\r\n        }\r\n    }\r\n    \r\n    noUserError();\r\n    \r\n    return false;\r\n}\r\n\r\nreturn [\r\n    'sentry' => true,\r\n    \/**\r\n     * Code here is executed prior to loading the page.\r\n     * \r\n     * @return string\r\n     *\/\r\n    'pageload_hook' => function () {\r\n        $attributes = \\Session::get('saml_attributes');\r\n        $original = \\Session::get('saml_original');\r\n        \r\n        if (!empty($_GET['attrs'])) {\r\n            header('Content-Type: application\/json');\r\n            echo json_encode($attributes);\r\n            exit;\r\n        }\r\n        \r\n        if (!empty($_GET['test'])) {\r\n            DB::statement(\"UPDATE [dbo].[iw_tableauviz_dashboards]\r\n            SET\r\n             [created_at] = CAST([created_at] AS datetime2(0)),\r\n             [updated_at] = CAST([updated_at] AS datetime2(0))\r\n            WHERE 1=1;\");\r\n            DB::statement(\"UPDATE [dbo].[backend_users]\r\n            SET\r\n             [created_at] = CAST([created_at] AS datetime2(0)),\r\n             [updated_at] = CAST([updated_at] AS datetime2(0)),\r\n             [last_login] = CAST([last_login] AS datetime2(0)),\r\n             [deleted_at] = NULL\r\n            WHERE 1=1;\